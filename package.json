{
  "name": "c3-lib",
  "private": true,
  "version": "0.0.1",
  "description": "Couch3ater Library",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run lint && npm-run-all --parallel serve watch",
    "test": "stencil test --spec --e2e",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "generate": "stencil generate",
    "serve": "stencil build --prerender && node build && stencil build --dev --watch --serve",
    "watch": "onchange \"src/components/**/*.scss\" \"src/html/**/*.html\" \"src/**/*{.ts,.tsx}\" -- npm run lint && npm run build:dev",
    "build:prod": "stencil build --prod --prerender && npm run wait && node build",
    "build:dev": "stencil build --dev",
    "build:render": "stencil build --prerender",
    "update:browserdb": "npx browserslist@latest --update-db",
    "update:stencil": "npm install @stencil/core@latest --save-exact",
    "wait": "ping -n 3 127.0.0.1 >NUL"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "0.4.0",
    "@stencil/router": "1.0.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "npm-run-all": "4.1.5",
    "onchange": "7.1.0",
    "prettier": "2.5.0",
    "rollup": "2.60.2"
  },
  "dependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@stencil/core": "2.11.0",
    "@stencil/postcss": "2.1.0",
    "@stencil/sass": "1.5.2",
    "autoprefixer": "10.4.0",
    "axios": "0.24.0",
    "bootstrap": "5.1.3",
    "eslint": "7.32.0",
    "express": "4.18.2",
    "fs": "0.0.1-security",
    "jsdom": "19.0.0",
    "ncp": "2.0.0",
    "path": "0.12.7",
    "pretti": "2.0.1",
    "resize-observer": "1.0.4",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "stencil-router-v2": "0.6.0",
    "typescript": "4.4.4"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 63",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "Edge >= 79",
    "not Explorer <= 11"
  ]
}
